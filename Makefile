SRC = prog_remove.c\

OBJ = $(SRC:.c=.o)

CC	= cc

PROG= rm_prog

all : $(PROG)

$(PROG) : $(OBJ)

	$(CC) $(OBJ) -o $(PROG)

clean : 

	rm -rf $(PROG) $(OBJ)